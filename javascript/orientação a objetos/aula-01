<!DOCTYPE html>
<html>
    <head>
        <title>orientação a obj</title>
    </head>
    <body>
        <script>
            /*
            -> isso é um objeto!
            var pessoa = {
                'nome': 'rebs',
                'idade': '16',
                'main': 'dva'
            };*/

            /*
            -> pode também existir uma array de objetos
            */
           var pessoas = [{
            'nome': 'rebs',
            'idade': 16,
            'main': 'dva'
           },
           {
            'nome': 'gabs',
            'idade': 20,
            'main': 'amô da rebs'
           }
        ]

           console.log(pessoas[1].main);

        </script>
    </body>
</html>
