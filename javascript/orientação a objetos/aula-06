    <script>

        //colocando assim, ele vai mostrar a data que eu colocar no constructor
        var date = new Date('07/20/1980');
        var date2 = new Date('07/31/1980');
        
        //como calcular diferen√ßa de data!
        var diference = (date2.getTime() - date.getTime()) / (1000 * 3600 * 24);

        console.log(diference);

    </script>

